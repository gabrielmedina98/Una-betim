	System.out.println("--------Inicio do programa----------");

        double peso, media=0, soma=0;
        int idade=0, cont=0;
         for (int i = 0; i < 7; i++) {
             System.out.println("Digite a idade da" + (i+1)+ "pessoa");
             idade = leia.nextInt();
             soma =  soma+idade;
            
        }
         
         media = soma/7;
        System.out.println(media);
         
        for (int i = 0; i < 7; i++) {
             System.out.println("Digite o peso da " + (i+1)+ "pessoa");
             peso = leia.nextInt();
             if(peso>90)
             cont++;
            
        }
        System.out.println("Pessoas com peso acima de 90kg :" + cont);
         
	
	
	
	
	
	
